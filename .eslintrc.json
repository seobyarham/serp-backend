{
  "env": {
    "node": true,
    "es2020": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-explicit-any": "off",
    "no-empty": "off",
    "no-inner-declarations": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/explicit-any": "off",
    "@typescript-eslint/no-explicit-any": "off"
  }
}
